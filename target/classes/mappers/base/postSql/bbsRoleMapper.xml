<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bbsRole">
	<select id="list" parameterType="hashMap" resultType="bbsRoleDTO">
		select 
			role_id,
			role_title,
			role_step
		from
		tb_bbs_role
	</select>
</mapper>